{
    "editor.mouseWheelZoom": true,
    "gitlens.advanced.messages": {
        // "suppressResultsExplorerNotice": true,
        // "suppressShowKeyBindingsNotice": true,
    },
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "editor.lineHeight": 25,
    "extensions.ignoreRecommendations": false,
    "printcode.fontSize": 10,
    "terminal.integrated.rendererType": "dom",
    // Vim plugin
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-h>": true,
        "<C-j>": true,
        "<C-k>": true,
        "<C-l>": true,
        "<C-w>": true,
        "<A-Up>": false,
        "<A-Down>": false,
        "<C-o>": false,
    },
    "vim.useSystemClipboard": true,
    "vim.easymotionMarkerForegroundColorTwoChar": "#efeeed",
    "vim.easymotionMarkerForegroundColorOneChar": "#efeeed",
    "vim.easymotionMarkerBackgroundColor": "#cc2c7f",
    "vim.easymotionMarkerFontWeight": "bold",
    "vim.easymotionMarkerFontSize": "16",
    "vim.easymotionMarkerHeight": 20,
    "vim.easymotionMarkerFontFamily": "Source Code Pro for Powerline",
    "vim.easymotionMarkerWidthPerChar": 12,
    "vim.foldfix": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "<leader>",
                "b",
                "d"
            ],
            "commands": [
                ":q"
            ],
        },
        {
            "before": [
                "<C-h>"
            ],
            "after": [
                "<C-w>",
                "h"
            ]
        },
        {
            "before": [
                "<C-l>"
            ],
            "after": [
                "<C-w>",
                "l"
            ]
        },
        {
            "before": [
                "<C-j>"
            ],
            "after": [
                "<C-w>",
                "j"
            ]
        },
        {
            "before": [
                "<C-k>"
            ],
            "after": [
                "<C-w>",
                "k"
            ]
        },
        {
            "before": [
                "("
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                ")"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
    ],
    "vim.statusBarColorControl": false,
    "vim.statusBarColors.normal": "#56c61d",
    "vim.statusBarColors.insert": "#41bdea",
    "vim.statusBarColors.visual": "#ffb52d",
    "vim.statusBarColors.visualblock": "#f95a3e",
    "vim.statusBarColors.visualline": "#fc8c41",
    "breadcrumbs.enabled": true,
    "files.autoSave": "off",
    "workbench.iconTheme": "material-icon-theme",
    "git.detectSubmodules": false,
    "git.detectSubmodulesLimit": 0,
    "editor.minimap.enabled": false,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "entity.name.scope-resolution.parameter.cpp",
                "settings": {
                    "foreground": "#a9b2c3",
                }
            },
            {
                "scope": "entity.name.scope-resolution.cpp",
                "settings": {
                    "foreground": "#a9b2c3",
                }
            },
            {
                "scope": "storage.type.class.doxygen.cpp",
                "settings": {
                    "foreground": "#5f6672",
                    "fontStyle": "bold"
                }
            },
            {
                "scope": "comment.block.documentation.cpp variable.parameter",
                "settings": {
                    "foreground": "#5f6672",
                }
            },
            {
                "scope": "support.type",
                "settings": {
                    "foreground": "#56b6c2",
                }
            },
            {
                "scope": "entity.name.type",
                "settings": {
                    "foreground": "#56b6c2",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.namespace",
                "settings": {
                    "foreground": "#a9b2c3",
                }
            },
            {
                "scope": "entity.name.operator.overloadee",
                "settings": {
                    "foreground": "#e06c75",
                }
            },
            {
                "scope": "variable.other.enummember",
                "settings": {
                    "foreground": "#d19a66",
                    "fontStyle": ""
                }
            },
            {
                "scope": "variable.other.global",
                "settings": {
                    "foreground": "#89b6ff",
                    "fontStyle": ""
                }
            },
            {
                "scope": "entity.name.other.preprocessor.macro",
                "settings": {
                    "foreground": "#D19A66",
                    "fontStyle": ""
                }
            }
        ]
    },
